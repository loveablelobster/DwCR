<?xml version="1.0" ?>
<archive xmlns="http://example.org/dwcr/spec/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://example.org/dwcr/spec/ http://example.org/dwcr/spec//example_dwcr_spec.xsd">
	<core fieldsEnclosedBy="&quot;"
	      fieldsTerminatedBy=","
	      linesTerminatedBy="\r\n"
	      rowType="http://example.org/dwcr/terms/CoreItem">
		<files>
			<location>core_file.csv</location>
		</files>
		<id index="0"/>
		<field index="0" term="http://example.org/dwcr/terms/coreID"/>
		<field index="1" term="http://example.org/dwcr/terms/termB"/>
		<field index="2" term="http://example.org/dwcr/terms/termC"/>
		<field index="3" term="http://example.org/dwcr/terms/termD"/>
		<field index="4" term="http://example.org/dwcr/terms/termE"/>
		<field index="5" term="http://example.org/dwcr/terms/termF"/>
		<field index="6" term="http://example.org/dwcr/terms/termG"/>
		<field index="7" term="http://example.org/dwcr/terms/termH"/>
		<field index="8" term="http://example.org/dwcr/terms/termI"/>
		<field default="F default" term="http://example.org/dwcr/terms/termF"/>
		<field default="J default" term="http://example.org/dwcr/terms/termJ"/>
	</core>
	<extension fieldsEnclosedBy="&quot;" fieldsTerminatedBy="," linesTerminatedBy="\r\n" rowType="http://example.org/dwcr/terms/ExtensionItem">
		<files>
			<location>extension_file1.csv</location>
			<location>extension_file2.csv</location>
		</files>
		<coreid index="0"/>
		<field index="1" term="http://example.org/dwcr/terms/identifier"/>
		<field index="2" term="http://example.org/dwcr/terms/termK"/>
		<field index="3" term="http://example.org/dwcr/terms/termL"/>
		<field default="L default" term="http://example.org/dwcr/terms/termL"/>
		<field default="elements L default" term="http://example.org/elements/termL"/>
	</extension>
</archive>
