<?xml version="1.0" ?>
<archive xmlns="http://example.org/dwcr/spec/"
         xmlns:xs="http://www.w3.org/2001/XMLSchema"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://example.org/dwcr/spec/ http://example.org/dwcr/spec//example_dwcr_spec.xsd">
	<core fieldsEnclosedBy="&quot;"
	      fieldsTerminatedBy=","
	      linesTerminatedBy="\r\n"
	      rowType="http://example.org/dwcr/terms/CoreItem">
		<files>
			<location>core_file.csv</location>
		</files>
		<id index="0"/>
		<field index="0" term="http://example.org/dwcr/terms/coreID"/>
		<field index="1" term="http://example.org/dwcr/terms/itemNumber"/>
		<field index="2" term="http://example.org/dwcr/terms/emptyColumn"/>
		<field index="3" term="http://example.org/dwcr/terms/textColumn"/>
		<field index="4" term="http://example.org/dwcr/terms/mixedColumn"/>
		<field index="5" term="http://example.org/dwcr/terms/numericColumn"/>
		<field index="6" term="http://example.org/dwcr/terms/dateColumn"/>
		<field default="column 4 default" term="http://example.org/dwcr/terms/mixedColumn"/>
		<field default="default value" term="http://example.org/dwcr/elements/emptyColumn"/>
	</core>
	<extension fieldsEnclosedBy="&quot;" fieldsTerminatedBy="," linesTerminatedBy="\r\n" rowType="http://example.org/dwcr/terms/ExtensionItem">
		<files>
			<location>extension_file1.csv</location>
			<location>extension_file2.csv</location>
		</files>
		<coreid index="0"/>
		<field index="1" term="http://example.org/dwcr/terms/identifier"/>
		<field index="2" term="http://example.org/dwcr/terms/coreItemNumber"/>
	</extension>
</archive>
